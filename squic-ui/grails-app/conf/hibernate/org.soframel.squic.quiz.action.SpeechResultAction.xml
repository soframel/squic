<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.soframel.squic.quiz.action.SpeechResultAction" table="SpeechResultAction">
        <id name="id" type="string" column="id">
            <generator class="native"/>
        </id>
        <component name="speechFile" class="org.soframel.squic.quiz.media.SoundFile">
            <property name="file" column="speechFile" type="string"/>
        </component>
    </class>
</hibernate-mapping>